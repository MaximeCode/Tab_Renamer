{
  "manifest_version": 3,
  "name": "Custom Tab Name - Smart Rename & Organize | BÊTA",
  "version": "1.0.0",
  "description": "Rename tabs with smart URL matching & phpMyAdmin DB detection. Bilingual (EN/FR) | CETTE EXTENSION EST POUR LES TESTS BÊTA",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Maxime BAUDE (@MaximeCode)",
  "homepage_url": "https://github.com/MaximeCode/Tab_Renamer"
}